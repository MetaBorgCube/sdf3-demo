program

class Counter extends Object {
   Num count
   Num count() {
     this.count = this.count + 1
   }
}

run
  let Counter c1 = new Counter
      Counter c2 = new Counter
      Counter c3 = new Counter
   in {
     c1.count();
     c2.count();
     c1.count();
     c2.count();
     c3.count();
     c1.count();
     c1.count * c2.count
   }